{% for flashMsg in app.session.flashbag.get('success') %}
    <div class="alert alert-dark alert-success">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        {{ flashMsg }}
    </div>
{% endfor %}

