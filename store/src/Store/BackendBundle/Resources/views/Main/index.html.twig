    </div>
    <hr class="no-grid-gutter-h grid-gutter-margin-b no-margin-t">
    <div class="row">

        <!-- 10. $SUPPORT_TICKETS ==========================================================================

                    Support tickets
        -->


        <div class="col-md-6">
            <div class="panel panel-success widget-support-tickets" id="dashboard-support-tickets">
                <div class="panel-heading">
                    <span class="panel-title"><i class="panel-title-icon fa fa-shopping-cart"></i>Dernières commandes</span>
                    <div class="panel-heading-controls">
                        <div class="panel-heading-text"><a href="#">
                                {%  set count_ord_p = 0 %}
                                {% for order in orders %}
                                    {% if order.state == 1 %}
                                        {% set count_ord_p = count_ord_p + 1 %}
                                    {% endif %}{% endfor %}{{ count_ord_p }} new orders</a></div>
                    </div>
                </div> <!-- / .panel-heading -->
                <div class="panel-body tab-content-padding">
                    <!-- Panel padding, without vertical padding -->
                    <div class="panel-padding no-padding-vr" style="overflow: hidden; width: auto; height: 300px;">
                        {% for order in orders %}
                            <div class="ticket">
                                {% if order.state == 0 %}
                                    <span class="label label-danger ticket-label">Rejected</span>
                                {% elseif order.state == 1 %}
                                    <span class="label label-warning ticket-label">Pending</span>
                                {% elseif order.state == 2 %}
                                    <span class="label label-success ticket-label">Completed</span>
                                {% endif %}
                                <a href="#" title="" class="ticket-title">Mobile App Problem<span>[#{{ order.id }}]</span></a>
								<span class="ticket-info">
									Ordered by <a href="#">{{ order.user.firstname ~' '~ order.user.lastname }}</a> in {{ order.address }}
                                    for {{ order.total }}€
								</span>
                            </div> <!-- / .ticket -->
                        {%  endfor %}
                    </div>
                </div> <!-- / .panel-body -->
            </div> <!-- / .panel -->
        </div>
        <!-- /10. $SUPPORT_TICKETS -->

        <!-- 11. $RECENT_ACTIVITY ==========================================================================

                    Recent activity
        -->

        <div class="col-md-6">
            <div class="panel panel-warning" id="dashboard-recent">
                <div class="panel-heading">
                    <span class="panel-title"><i class="panel-title-icon fa fa-comments text-danger"></i>Latest Comments</span>
                    <ul class="nav nav-tabs nav-tabs-xs">
                        <li class="active">
                            <a href="#dashboard-active-comments" data-toggle="tab">Actifs</a>
                        </li>
                        <li>
                            <a href="#dashboard-pending-comments" data-toggle="tab">En cours de modération</a>
                        </li>
                        <li>
                            <a href="#dashboard-disabled-comments" data-toggle="tab">Inactifs</a>
                        </li>
                    </ul>
                </div> <!-- / .panel-heading -->
                <div class="tab-content">

                    <!-- Comments widget -->

                    <!-- Without padding -->
                    <div class="widget-comments panel-body tab-pane no-padding fade active in" id="dashboard-active-comments">
                        <!-- Panel padding, without vertical padding -->
                        <div class="panel-padding no-padding-vr" style="overflow: hidden; width: auto; height: 300px;">
                                {% for com in coms %}
                                    {% if com.state == 1 %}
                                        <div class="comment">
                                            <img src="assets/demo/avatars/5.jpg" alt="" class="comment-avatar">
                                            <div class="comment-body">
                                                <div class="comment-by">
                                                    <a href="#" title="">{{ com.user.firstname ~' '~ com.user.lastname }}</a> commented on <a href="{{ path('store_backend_product_view',{id: com.product.id, name: com.product.title}) }}" title="">{{ com.product.title }}</a>
                                                </div>
                                                <div class="comment-text">
                                                    {{ com.content }}
                                                </div>
                                                <div class="comment-actions">
                                                    <a href="#"><i class="fa fa-pencil"></i>Edit</a>
                                                    <a href="#"><i class="fa fa-times"></i>Remove</a>
                                                    <span class="pull-right">{{ com.dateCreated|date('d/m/Y h:i') }}</span>
                                                </div>
                                            </div> <!-- / .comment-body -->
                                        </div> <!-- / .comment -->
                                    {% endif %}
                                {% endfor %}
                            </div>
                    </div> <!-- / .widget-comments -->

                    <!-- Threads widget -->

                    <!-- Without padding -->
                    <div class="widget-comments panel-body tab-pane no-padding fade" id="dashboard-pending-comments">
                        <div class="panel-padding no-padding-vr" style="overflow: hidden; width: auto; height: 300px;">
                                {% for com in coms %}
                                    {% if com.state == 0 %}
                                        <div class="comment">
                                            <img src="assets/demo/avatars/5.jpg" alt="" class="comment-avatar">
                                            <div class="comment-body">
                                                <div class="comment-by">
                                                    <a href="#" title="">{{ com.user.firstname ~' '~ com.user.lastname }}</a> commented on <a href="{{ path('store_backend_product_view',{id: com.product.id, name: com.product.title}) }}" title="">{{ com.product.title }}</a>
                                                </div>
                                                <div class="comment-text">
                                                    {{ com.content }}
                                                </div>
                                                <div class="comment-actions">
                                                    <a href="#"><i class="fa fa-pencil"></i>Edit</a>
                                                    <a href="#"><i class="fa fa-times"></i>Remove</a>
                                                    <span class="pull-right">{{ com.dateCreated|date('d/m/Y h:i') }}</span>
                                                </div>
                                            </div> <!-- / .comment-body -->
                                        </div> <!-- / .comment -->
                                    {% endif %}
                                {% endfor %}
                        </div>
                    </div> <!-- / .panel-body -->

                    <div class="widget-comments panel-body tab-pane no-padding fade" id="dashboard-disabled-comments">
                        <!-- Panel padding, without vertical padding -->
                        <div class="panel-padding no-padding-vr" style="overflow: hidden; width: auto; height: 300px;">
                            {% for com in coms %}
                                {% if com.state == 2 %}
                                    <div class="comment">
                                        <img src="assets/demo/avatars/5.jpg" alt="" class="comment-avatar">
                                        <div class="comment-body">
                                            <div class="comment-by">
                                                <a href="#" title="">{{ com.user.firstname ~' '~ com.user.lastname }}</a> commented on <a href="{{ path('store_backend_product_view',{id: com.product.id, name: com.product.title}) }}" title="">{{ com.product.title }}</a>
                                            </div>
                                            <div class="comment-text">
                                                {{ com.content }}
                                            </div>
                                            <div class="comment-actions">
                                                <a href="#"><i class="fa fa-pencil"></i>Edit</a>
                                                <a href="#"><i class="fa fa-times"></i>Remove</a>
                                                <span class="pull-right">{{ com.dateCreated|date('d/m/Y h:i') }}</span>
                                            </div>
                                        </div> <!-- / .comment-body -->
                                    </div> <!-- / .comment -->
                                {% endif %}
                            {% endfor %}
                        </div>
                    </div> <!-- / .widget-comments-inactifs -->
                </div>
            </div> <!-- / .widget-threads -->
        </div>
        <!-- /11. $RECENT_ACTIVITY -->
    </div>
    <hr class="no-grid-gutter-h grid-gutter-margin-b no-margin-t">
    <div class="row">

        <!-- 8. $MESSAGES_LIST_ALT =========================================================================

                        Alt messages list
        -->
        <div class="widget-messages-alt col-md-6">
            <div class="panel panel-danger">
                <div class="panel-heading">
                    <span class="panel-title"><i class="panel-title-icon fa fa-envelope"></i>Messages</span>
                </div> <!-- / .panel-heading -->
                <div class="panel-body padding-sm">
                    <div class="messages-list">
                        {% for msg in msgs %}
                            <div class="message">
                                <img src="assets/demo/avatars/2.jpg" alt="" class="message-avatar">
                                <a href="#" class="message-subject">{{ msg.title }}</a>
                                <div class="message-description">
                                    from <a href="#">{{ msg.user.firstname ~' '~ msg.user.lastname }}</a>
                                    <p>{{ msg.content }}</p>
                                    &nbsp;&nbsp;·&nbsp;&nbsp;
                                    {{ msg.dateCreated | date('d/m/Y h:i') }}
                                    &nbsp;&nbsp;·&nbsp;&nbsp;
