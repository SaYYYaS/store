        <div class="col-md-6">
            <div class="panel panel-success widget-support-tickets" id="dashboard-support-tickets">
                <div class="panel-heading">
                    <span class="panel-title"><i class="panel-title-icon fa fa-dollar"></i>Promotions et Réduction</span>
                </div> <!-- / .panel-heading -->
                <div class="panel-body tab-content-padding">
                    <!-- Panel padding, without vertical padding -->
                    <div class="panel-padding no-padding-vr" style="overflow: hidden; width: auto; height: 300px;">
                        {% for promo in business %}
                            {{ dump(promo.product) }}
                            <div class="ticket">
                                <span class="label label-success ticket-label"><i class="fa fa-check"></i>-10%</span>
                                <a href="#" title="" class="ticket-title">Product name<span>[#{{ promo.id }}]
                                            <span class="label">
                                                {% if(promo.type == 1) %}
                                                    {#{{ promo.product.price - promo.amount }}#}
                                                {% else %}
                                                    {#{{ promo.product.price *(1 - promo.amount) }}#}
                                                {% endif %}
                                                €</span>
                                        </span></a>
								<span class="ticket-info">
                                    <span> -- {{ promo.type == 1 ? "promotion" : "reduction" }} -- </span>
								</span>
                                <span class="ticket-message">
                                    {{ promo.message }}
                                </span>
                            </div> <!-- / .ticket -->
                        {%  endfor %}
