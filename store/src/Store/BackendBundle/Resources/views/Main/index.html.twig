                                        <span class="label label-default pull-right">Désactivé</span>
                                    {% elseif msg.state == 1 %}
                                        <span class="btn btn-primary label label-primary pull-right">Lire</span>
                                    {% elseif msg.state == 2 %}
                                        <span class="label label-success pull-right">Lu</span>
                                    {% endif %}
                                </div> <!-- / .message-description -->
                            </div> <!-- / .message -->
                        {% endfor %}
